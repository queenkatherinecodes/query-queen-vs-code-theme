{
    "name": "Query Queen Theme",
    "type": "dark",
    "colors": {
        // Main editor colors
        "editor.background": "#362F25",          // Lighter latte brown
        "editor.foreground": "#FFE4E1",          // Soft pink
        "activityBar.background": "#2A2319",     // Lighter dark brown
        "sideBar.background": "#2E271D",         // Lighter medium brown
        "statusBar.background": "#453B2F",       // Lighter mocha
        "titleBar.activeBackground": "#2A2319",  // Lighter dark brown
        "tab.activeBackground": "#362F25",       // Match editor
        "tab.inactiveBackground": "#2E271D",     // Slightly darker

        // Editor highlighting and selection
        "editor.selectionBackground": "#FF69B440",    // Light pink with opacity
        "editor.lineHighlightBackground": "#3D3529",  // Slightly lighter brown
        "editor.wordHighlightBackground": "#FF69B420",// Very light pink
        "editorCursor.foreground": "#FFB6C1",        // Light pink
        "editorWhitespace.foreground": "#453B2F",    // Lighter mocha

        // Terminal colors
        "terminal.background": "#362F25",        // Match editor background
        "terminal.foreground": "#FFE4E1",        // Soft pink
        "terminalCursor.foreground": "#FFB6C1",  // Light pink
        "terminal.selectionBackground": "#FF69B440",
        "terminal.ansiBlue": "#DDA0DD",         // Plum pink
        "terminal.ansiBrightBlue": "#FF69B4",   // Hot pink
        
        // Sidebar and lists
        "list.activeSelectionBackground": "#FF69B440",
        "list.hoverBackground": "#3D3529",
        "sideBarSectionHeader.background": "#2A2319",
        
        // Additional UI elements
        "focusBorder": "#FF69B4",              // Hot pink for focus borders
        "editorLink.activeForeground": "#FF69B4",
        "textLink.foreground": "#FF69B4",
        "textLink.activeForeground": "#DDA0DD",
        "editorBracketMatch.background": "#FF69B440",
        "editorBracketMatch.border": "#FF69B4"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#D8BFD8"  // Thistle pink
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#FFB6C1"  // Light pink
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF69B4"  // Hot pink
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#DDA0DD"  // Plum pink
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#FFC0CB"  // Pink
            }
        },
        {
            "name": "Classes/Types",
            "scope": ["entity.name.type", "entity.name.class", "support.type"],
            "settings": {
                "foreground": "#FF1493"  // Deep pink
            }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#FFE4E1"  // Misty rose
            }
        },
        {
            "name": "Operators and Brackets",
            "scope": [
                "keyword.operator",
                "punctuation.definition.bracket",
                "meta.brace",
                "punctuation.definition.block",
                "punctuation.definition.parameters"
            ],
            "settings": {
                "foreground": "#FFB6C1"  // Light pink
            }
        }
    ]
}